{
  "name": "nuxt3-tailwind-breakpoints",
  "version": "0.1.1",
  "description": "Show Tailwind CSS Breakpoints in Nuxt 3",
  "repository": "kswedberg/nuxt3-tailwind-breakpoints",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "cd playground && nuxi dev",
    "dev:build": "cd playground && nuxi build",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.2",
    "interactjs": "^1.10.18",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.2",
    "@nuxt/test-utils": "^3.7.3",
    "@types/node": "^18.17.15",
    "changelogen": "^0.5.5",
    "eslint": "^8.49.0",
    "eslint-config-kswedberg": "^5.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "nuxt": "^3.7.3",
    "vitest": "^0.33.0"
  }
}
